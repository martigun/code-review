						HS.Exp "A#" & CurrRollAcct_Subs & ".C3#Subs_BegBal.C1#" & dSubsRFO(i) & RFO_Calcs & _
							" = A#" & CurrRollAcct_Subs & ".C3#Subs_BegBal.C4#Rollforward.P#Prior.C1#" & dSubsRFO(i) & RFO_Tops_1Sub & source_value
					End If  'period #
					' Calculate the rollforward difference account
					HS.Clear "A#" & CurrRollAcct_Subs & ".C3#Subs_Undist.C1#" & dSubsRFO(i) & RFO_Calcs
					HS.Exp "A#" & CurrRollAcct_Subs & ".C3#Subs_Undist.C1#" & dSubsRFO(i) & RFO_Calcs & _
						" = A#" & CurrRollAcct_Subs & ".C1#" & dSubsRFO(i) & TopsNoC1 & source_value &_
						" - A#" & CurrRollAcct_Subs & ".C3#Subs_BegBal.C4#RollForward.C1#" & dSubsRFO(i) & RFO_Tops_1Sub & source_value & _
						" - A#" & CurrRollAcct_Subs & ".C3#Subs_Mvmt.C4#RollForward.C1#" & dSubsRFO(i) & RFO_Tops_1Sub & source_value
				Next
			Next 'RollAcctLoop_Subs
		End If 'povValue
	End If 'pov_entity
	
	'11. Undistributed for AC Procurement (M-16-048)
	If pov_entity = "C_DAL" and pov_value = "<Entity Currency>" Then
		
		'Clear the undist
		HS.Clear "A#1670180_M.C3#ACP_Undist" & RFO_Calcs_DL
		
		'Set undist equal to CashAdds less AC_Proc_Tot
		HS.Exp "A#1670180_M.C3#ACP_Undist" & RFO_Calcs_DL & " = - A#1670180_M.C3#AC_Proc_Tot.C4#RollForward" & RFO_Tops_DL & _
															" + A#1670180_M.C3#FltGrd_CashAdd.C4#RollForward" & RFO_Tops_DL & _
															" + A#A63000.C3#FltGrd_CashAdd.C4#RollForward" & RFO_Tops_DL & _
															" + A#SA66470.C3#FltGrd_CashAdd.C4#RollForward" & RFO_Tops_DL
	End If

	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Balance Sheet Movement                                                   *
	'*  Rule Number: C008                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G. Broadhurst                                                            *
	'* Date Created: 15Apr2015                                                                *
	'*                                                                                        *
	'*      Purpose: To help facilitate the cash flow process and to determine the FX impact  *
	'*               on each balance sheet account we set up members in Custom 3 to hold the  * 
	'*				 Opening Balance, Movement, CTA and Ending Balance.                       *
	'*                                                                                        *
	'******************************************************************************************

	'The rule needs to start in 2014 since the application's first year is 2013
	'Updated M-16-048
	If pov_scenario = "Actual" and pov_year > "2013" Then
	
		'The rule runs if the entity is base or it translates or if the Value dimension is Elimination
		If ( pov_base = TRUE or pov_trans = TRUE or pov_value = "[Elimination]" )  Then
			'If we are in the first period pull the ending balance from the last period of the prior year
			If PeriodIsFirst = TRUE Then
				
				'Balance Sheet Accounts
				aBalSheetAccounts = HS.Account.List("Balance_Sheet","[Base]")			'Create a list of Balance Sheet Accounts
				
				HS.Clear "C3#BS_BegBal"
				HS.Clear "C3#BS_Mvmt"
				For i = LBound(aBalSheetAccounts) to UBound(aBalSheetAccounts)			'For each account in the list	
					HS.Exp "A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal = A#" & aBalSheetAccounts(i) & ".C3#TopC3.P#LAST.Y#PRIOR"
					HS.Exp "A#" & aBalSheetAccounts(i) & ".C3#BS_Mvmt.W#YTD = A#" & aBalSheetAccounts(i) & ".C3#TopC3.W#YTD" & _
																				  "- A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal.W#YTD" & _
																				  "- A#" & aBalSheetAccounts(i) & ".C3#BS_CTA.W#YTD"
				Next
				
			'To remain in the current sub-cube we just need to continue to pull forward the opening balance
			'from the prior period rather than to keep pulling back from the prior year.
			Else
				
				'Balance Sheet Accounts
				aBalSheetAccounts = HS.Account.List("Balance_Sheet","[Base]")			'Create a list of Balance Sheet Accounts

				HS.Clear "C3#BS_BegBal"
				HS.Clear "C3#BS_Mvmt"
				For i = LBound(aBalSheetAccounts) to UBound(aBalSheetAccounts)			'For each account in the list	
					HS.Exp "A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal = A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal.P#PRIOR"
					HS.Exp "A#" & aBalSheetAccounts(i) & ".C3#BS_Mvmt.W#YTD = A#" & aBalSheetAccounts(i) & ".C3#TopC3.W#YTD" & _
																				  "- A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal.W#YTD" & _
																				  "- A#" & aBalSheetAccounts(i) & ".C3#BS_CTA.W#YTD"
				Next
				
			End If 'Period is First
				
			If pov_value = "[Elimination]" Then
				
				'Balance Sheet Accounts
				aBalSheetAccounts = HS.Account.List("Balance_Sheet","[Base]")			'Create a list of Balance Sheet Accounts

				HS.Clear "C3#BS_CTA"
				HS.Clear "C3#BS_Mvmt"
				For i = LBound(aBalSheetAccounts) to UBound(aBalSheetAccounts)			'For each account in the list	
					HS.Exp "A#" & aBalSheetAccounts(i) & ".C3#BS_Mvmt.W#YTD = A#" & aBalSheetAccounts(i) & ".C3#TopC3.W#YTD" & _
																				  "- A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal.W#YTD" & _
																				  "- A#" & aBalSheetAccounts(i) & ".C3#BS_CTA.W#YTD"
				Next

			End If 'The Entity is base but does not translate or Elimination node

		End If 'If ( pov_base = TRUE or pov_value = "[Elimination]" )  Then

		
'		'For the historical accounts (equity and investments) we want to recalculate the movement, as the Beginning
'		'and Ending balances are held at historical rates.
'		If pov_trans = TRUE Then
'
'			vHistAccnts = HS.Account.List("S350000","[Base]")
'			HS.Clear "C3#BS_Mvmt"
'			For i = LBound(vHistAccnts) to UBound (vHistAccnts)		
'				HS.Exp "A#" & vHistAccnts(i) & ".C3#BS_Mvmt.W#YTD = A#" & vHistAccnts(i) & ".C3#TopC3.W#YTD" & _
'																		 "- A#" & vHistAccnts(i) & ".C3#BS_BegBal.W#YTD" & _ 
'																		 "- A#" & vHistAccnts(i) & ".C3#BS_CTA.W#YTD"
'			Next
'		
'		End If

		
		'This code only runs if the entity translates and captures the FX effect.
		If pov_trans = TRUE Then
		
			'Balance Sheet Accounts
			aBalSheetAccounts = HS.Account.List("Balance_Sheet","[Base]")			'Create a list of Balance Sheet Accounts

			HS.Clear "C3#BS_CTA"
			For i = LBound(aBalSheetAccounts) to UBound(aBalSheetAccounts)			'For each account in the list	
				HS.Exp "A#" & aBalSheetAccounts(i) & ".C3#BS_CTA.W#YTD = A#" & aBalSheetAccounts(i) & ".C3#TopC3.W#YTD" & _
																			"- A#" & aBalSheetAccounts(i) & ".C3#BS_BegBal.W#YTD" & _
																			"- A#" & aBalSheetAccounts(i) & ".C3#BS_Mvmt.W#YTD"
			Next
			
		End If 'The entity is translating
	
	End If	'If pov_scenario = "Actual" and pov_year > "2013" Then

	
	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Cash Flow                                                      	      *
	'*  Rule Number: C007                                                                    *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G. Martin                                                                *
	'* Date Created: 28Jun2015                                                                *
	'*                                                                                        *
	'*      Purpose: Rules to calculate the Cash Flow format	                              *
	'*                                                                                        *
	'******************************************************************************************
	
	'Updated M-16-048
	If pov_scenario = "Actual" Then
	If pov_year > "2014" Then 'Operate from Jan 2015 forward
		'GAAP CASH Flows
		
		'BEGIN_OPERATE
		'CASH FLOWS FROM OPERATING ACTIVITIES:

		'Cash Flow Net Income Loss
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01200" & RFO_CF_DL
			HS.Exp "A#CF01200" & RFO_CF_DL & " = A#SL92610.C3#Eqty_NetIncome.C4#Rollforward" & RFO_Tops_DL
		End If
		
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01800" & RFO_CF_DL
			HS.Exp "A#CF01800" & RFO_CF_DL & " = A#SL92610.C3#Eqty_NetIncome.C4#Rollforward" & RFO_Tops_DL
			'HS.Exp "A#CF07600" & RFO_CF_DL & " = - A#A29000.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If
		
		
		'BEGIN_ADJUSTMENTS

		'Adjustments to reconcile net income to net cash provided by operating activities:
		'Cumu effect of change in acctg principle
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01400" & RFO_CF_DL
			HS.Exp "A#CF01400" & RFO_CF_DL & " = A#SL92610.C3#Eqty_CumuAcctg.C4#Rollforward" & RFO_Tops_DL
		End If

		'Asset writedowns and special charges

		'Fleet - Accelerated depreciation
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01600" & RFO_CF_DL
			HS.Exp "A#CF01600" & RFO_CF_DL & " = - A#7001100.V#<Entity Curr Total>.C3#SP_Accel_Depr.E#P_DALINC" & TopsNoC3
		End If

		'Fleet - Impairment
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01800" & RFO_CF_DL 'RFO_Delta
			HS.Exp "A#CF01800" & RFO_CF_DL & " = - A#SA66810.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" - A#SA87610.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" - A#SA27710.C3#TOC_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" - A#SA65810.C3#TOC_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" - A#SA87650.C3#TOC_WD_Impair.C4#Rollforward" & RFO_Tops_DL
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01800" & RFO_CF_0256
			HS.Exp "A#CF01800" & RFO_CF_0256 & " = - A#SA27710.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0256 & _
												" - A#SA66910.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0256
		End If
		
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF01800" & RFO_CF_0120
			HS.Exp "A#CF01800" & RFO_CF_0120 & " = - A#SA27710.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0120 & _
												" - A#SA66910.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0120
		End If
		
		'Added M-16-042
		'Removed M-16-047
		' If pov_entity = "C_DAL" Then
			' HS.Clear "A#CF01800" & RFO_CF_0160
			' HS.Exp "A#CF01800" & RFO_CF_0160 & " = - A#SA27710.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0160 & _
												' " - A#SA66910.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0160
		' End If

		'Fleet - Remaining lease payments
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02000" & RFO_CF_DL
			HS.Exp "A#CF02000" & RFO_CF_DL & " = A#2190709.C3#Restr_SpecNC.C4#Rollforward" & RFO_Tops_DL & _
											" + A#SL75210.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
											" + A#2290186.C3#Restr_SpecNC.C4#Rollforward" & RFO_Tops_DL
		End If


		'Fleet - Lease return costs
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02200" & RFO_CF_DL
			HS.Exp "A#CF02200" & RFO_CF_DL & " = A#SL68810.C3#Restr_SpecNC.C4#Rollforward" & RFO_Tops_DL
		End If
		
		'Fleet - Other non cash (N/A)

		'Facility - Impairment
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02600" & RFO_CF_DL
			HS.Exp "A#CF02600" & RFO_CF_DL & " = - A#SA68010.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" - A#A73500.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" - A#SA87630.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL & _
							" + A#SL75270.C3#FltGrd_WD_Impair.C4#Rollforward" & RFO_Tops_DL
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02600" & RFO_CF_0700
			HS.Exp "A#CF02600" & RFO_CF_0700 & " = - A#SA68010.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0700

		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02600" & RFO_CF_0701
			HS.Exp "A#CF02600" & RFO_CF_0701 & " = - A#SA68010.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0701
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02600" & RFO_CF_0116
			HS.Exp "A#CF02600" & RFO_CF_0116 & " = - A#SA67270.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0116
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02600" & RFO_CF_0256
			HS.Exp "A#CF02600" & RFO_CF_0256 & " = - A#SA67270.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0256
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02600" & RFO_CF_0120
			HS.Exp "A#CF02600" & RFO_CF_0120 & " = - A#SA67270.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0120
		End If
		
		'Facility - Remaining lease payments
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF02800" & RFO_CF_DL 'RFO_Total
			HS.Exp "A#CF02800" & RFO_CF_DL & " = A#2190695.C3#Restr_SpecNC.C4#Rollforward" & RFO_Tops_DL & _
							" + A#2290180.C3#Restr_SpecNC.C4#Rollforward" & RFO_Tops_DL
		End If

		'Facility - Other non cash (N/A)
		'Added M-16-047
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF03000" & RFO_CF_0160
			HS.Exp "A#CF03000" & RFO_CF_0160 & " = - A#SA68010.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0160
		End If
		
		'Added M-17-014
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF03000" & RFO_CF_0110
			HS.Exp "A#CF03000" & RFO_CF_0110 & " = - A#SA68010.C3#Subs_WD_Impair.C4#Rollforward" & RFO_Tops_0110
		End If
		
		'Other special charges (Journal Entry)

		'Goodwill impairment
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF03400" & RFO_CF_DL
			HS.Exp "A#CF03400" & RFO_CF_DL & " = - A#A70500.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If

		'Depreciation and amortization

		'Flight
		'Flight depreciation, total
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05000" & RFO_CF_DL
			HS.Exp "A#CF05000" & RFO_CF_DL & " = - A#SA66470.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05000" & RFO_CF_0256
			HS.Exp "A#CF05000" & RFO_CF_0256 & " = - A#SA66910.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0256
		End If

		'Less accelerated depreciation
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05200" & RFO_CF_DL
			HS.Exp "A#CF05200" & RFO_CF_DL & " = A#7001100.V#<Entity Curr Total>.C3#SP_Accel_Depr.E#P_DALINC" & TopsNoC3
		End If
		
		'Expendable parts
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05600" & RFO_CF_DL
			HS.Exp "A#CF05600" & RFO_CF_DL & " = - A#SA27710.C3#TOC_Depr_Amort.C4#Rollforward" & RFO_Tops_DL
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05600" & RFO_CF_0256
			HS.Exp "A#CF05600" & RFO_CF_0256 & " = - A#SA27710.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0256
		End If

		'Repairables, rotables, and LLPs
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05800" & RFO_CF_DL
			HS.Exp "A#CF05800" & RFO_CF_DL & " = - A#SA65810.C3#TOC_Depr_Amort.C4#Rollforward" & RFO_Tops_DL
		End If
		
		'Added M-18-012
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF05800" & RFO_CF_0160
			HS.Exp "A#CF05800" & RFO_CF_0160 & " = - A#SA65810.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0160
		End If

		'Capital leases
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06000" & RFO_CF_DL
			HS.Exp "A#CF06000" & RFO_CF_DL & " = - A#SA66610.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL & _
										" - A#SA67410.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL
		End If

		'Ground
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06200" & RFO_CF_DL
			HS.Exp "A#CF06200" & RFO_CF_DL & " = - A#SA67270.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL
		End If

		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06200" & RFO_CF_0116
			HS.Exp "A#CF06200" & RFO_CF_0116 & " = - A#SA67270.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0116
		End If
		
		'Added M-16-047
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06200" & RFO_CF_0160
			HS.Exp "A#CF06200" & RFO_CF_0160 & " = - A#SA68010.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0160
		End If		
		
		'Added M-17-014
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06200" & RFO_CF_0110
			HS.Exp "A#CF06200" & RFO_CF_0110 & " = - A#SA68010.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0110
		End If		
	
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06200" & RFO_CF_0256
			HS.Exp "A#CF06200" & RFO_CF_0256 & " = - A#SA67270.C3#Subs_DeprAmort.C4#Rollforward" & RFO_Tops_0256
		End If

		'Non-operating property
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06400" & RFO_CF_DL
			HS.Exp "A#CF06400" & RFO_CF_DL & " = - A#SA87610.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL & _
										" - A#SA87630.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL & _
										" - A#SA87650.C3#TOC_Depr_Amort.C4#Rollforward" & RFO_Tops_DL
		End If

		'Amortization on intangibles
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF06600" & RFO_CF_DL
			HS.Exp "A#CF06600" & RFO_CF_DL & " = - A#A73500.C3#FltGrd_DeprAmort.C4#Rollforward" & RFO_Tops_DL
		End If

		'Deferred income taxes
		'Deferred tax asset - Current (Change)
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF07600" & RFO_CF_DL
			HS.Exp "A#CF07600" & RFO_CF_DL & " = - A#A29000.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If

		'Deferred tax asset - Noncurrent (Change)
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF07800" & RFO_CF_DL
			HS.Exp "A#CF07800" & RFO_CF_DL & " = - A#A74000.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If

		'Deferred tax liability - Noncurrent (Change)
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF08000" & RFO_CF_DL
			HS.Exp "A#CF08000" & RFO_CF_DL & " = A#L62500.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If

		'AOCI - Taxes (Change)
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF08200" & RFO_CF_DL
			HS.Exp "A#CF08200" & RFO_CF_DL & " = A#SL91910.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If	
			
		'Amortization of debt discount (premium)
		'Stock compensation
