'****************************************************************************************************************

'C001 - Pull YTD Net Income into Current Year Earnings
'C002 - Retained Earnings Beginning Balance
'C003 - Earnings Per Share (EPS)
'C004 - Balance Sheet Out of Balance (OOB)
'C005 - Calculate CTA on the Balance Sheet
'C006 - Rollforwards
'C007 - Cash Flow
'C008 - Balance Sheet Movement
'C009 - Statistical Calculations
'C010 - Monroe Collapse (2015 version)
'C011 - Calculate CTA on the Balance Sheet
'C012 - Copy Data to Cash Flow Entities
'C013 - Monroe Collapse (2016 forward version)
'C014 - USD Overrides


'****************************************************************************************************************
Sub Calculate()
'****************************************************************************************************************
'Define some variables for the POV that we will use throughout this file
 pov_entity = HS.Entity.Member   		'Current entity calculating in the rules
 pov_parent    = HS.Parent.Member	    'parent of current pov entity
 isABase = HS.Entity.IsBase("","")		'True if the current Entity is a base
 pov_value = HS.Value.Member    		'Current value calculating in the rules
 pov_scenario = HS.Scenario.Member  	'Current scenario calculating in the rules
 pov_period = HS.Period.Number         	'Period number (1), not Period Label (Oct)
 pov_per_member = HS.Period.Member      'Period Label (Oct)
 pov_year = HS.Year.Member    			'Current year calculating in the rules
 pov_trans = HS.Value.IsTransCur   		'Boolean indicating if the current value member is translated (Parent Currency)
 pov_transadj = HS.Value.IsTransCurAdj 	'Boolean indicating if the current value member is translated (Parent Curr Adjs)
 pov_base = HS.Entity.IsBase("","")  	'Boolean indicating if the current entity is a base member
 pov_defcur = HS.Entity.DefCurrency("") 'Default currency of the current entity calculating in the rules
 PeriodIsFirst = HS.Period.IsFirst  	'Boolean indicating if the current period is the first of the year (Jan)
 pov_parent = HS.Entity.UD3(pov_entity) 'Stores what is in the UD3 field for the entity
 pov_EntUD1 = HS.Entity.UD1("")  		'Stores what is in the UD1 field for the entity
 pov_ScenUD1 = HS.Scenario.UD1("")  	'Stores what is in the UD1 field for the scenario
 pov_entity = HS.Entity.Member			'Current entity calculating in the rules

'****************************************************************************************************************
  'If you ask for the currency of the value [None] you get an error - so don't ask
    If pov_value <> "[None]" Then
    	translate_to = HS.Value.Currency    'This determines what currency we are translating to at the moment
    End If
'****************************************************************************************************************

' Many calculations are run at the entity and parent level.  We typically write
' rules to populate the <Entity Currency> and <Parent Currency>.  The issue is
' that the data that we want to draw from is in the <Entity Curr Total> and
' the [Parent Total].  These statements help to determine the right hand side
' of the equation.

    'If at the Entity Currency
    If pov_value = "<Entity Currency>" Then
        'Make the data source Entity Currency Total
        source_value = ".V#<Entity Curr Total>"

    'If at Parent Currency
    ElseIf HS.Value.IsTransCur = True Then
        'Take the currency (like USD) and add "Total" to the end to get the new
        ' source ie: USD becomes .V#USD Total
        source_value = ".V#" & translate_to & " Total"

    'If at the Proportion node
    ElseIf pov_value = "[Proportion]" Then
        'Make the data source the Contribution Total
        source_value = ".V#[Contribution Total]"

    'If at the Elimination node		
	ElseIf pov_value = "[Elimination]" Then
		'Make the data source the Elimination
		source_value = ".V#[Elimination]"
		
    End If  'Value

	
	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Pull YTD Net Income into Retained Earnings                               *
	'*  Rule Number: C001                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G. Broadhurst                                                            *
	'* Date Created: 9Apr2015                                                                 *
	'*                                                                                        *
	'*      Purpose: Calculate current Year Earnings in Equity                                *
	'*	             				                             	                          *
	'*                                                                                        *
	'******************************************************************************************

	'Updated M-16-048
	If pov_scenario = "Actual" Then
		
		If pov_entity ="C_DAL" Then
			
			'Do Nothing if this is a Cash Flow Entity
			HS.Clear "A#X280001" & Calc_Load
		
		Else
			
			'Set CY earnings if not a CF entity
			HS.Clear "A#X280001" & Calc_Load
			HS.Exp "A#X280001" & Calc_Load & "= A#R99900.W#YTD" & Tops
			
		End If
		
	End If
	
	'Added M-17-044 (for Pro Forma reporting)
	If pov_scenario = "Actual" Then
		
		If pov_entity ="C_DAL" Then
			
			'Do Nothing if this is a Cash Flow Entity
			HS.Clear "A#X280001" & Calc_Load_PRO
		
		Else
			
			'Set CY earnings if not a CF entity
			HS.Clear "A#X280001" & Calc_Load_PRO
			HS.Exp "A#X280001" & Calc_Load_PRO & "= A#R99900.W#YTD" & Tops_AcPRO
			
		End If
		
	End If
	
	


	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Retained Earnings Beginning Balance                                      *
	'*  Rule Number: C002                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G. Broadhurst                                                            *
	'* Date Created: 9Apr2015                                                                 *
	'* Date Updated: 18Feb2016                                                                *
	'*                                                                                        *
	'*      Purpose: To set the Opening Retained Earnings Account equal to the prior          *
    '*	             year's total Retained Earnings                                           *
	'*                                                                                        *
	'*       Update: This rule was updated on 18Feb2016 to fire only on base entities and     *
    '*	             only at <Ent Curr>. Also the data to pull from was adjusted to pull      *
    '*	             from source at <Ent Curr Tot>, so that any JE activity is included.      *
	'*                                                                                        *
	'******************************************************************************************

	'Updated M-16-048
	If pov_scenario = "Actual" Then
	
		'Only run on base entities and only run on Entity Currency
		If pov_year > "2013" And isABase = TRUE And pov_value = "<Entity Currency>" Then
		
			If pov_entity ="C_DAL" Then
			
				'Do Nothing if it is a CF entity
				HS.Clear "A#X280002" & Calc_Load
				
			Else
				
				'Set up the retained earnings beginning balance
				If pov_period = 1 Then

					HS.Clear "A#X280002" & Calc_Load
					HS.Exp "A#X280002" & Calc_Load & " = P#Prior.A#L85200" & Tops & source_value

				Else
					
					HS.Clear "A#X280002" & Calc_Load
					HS.Exp "A#X280002" & Calc_Load & " = P#Prior.A#X280002" & Tops & source_value

				End If
					
			End If

		End If

	End If
	
	'Added M-17-026
	'Special rules for USD override of X280002 (run for 2017 going forward)
	If pov_scenario = "Actual" And pov_year > "2016" And pov_trans = TRUE and pov_value = "USD" Then
	
		'If Jan...
		If pov_period = 1 then
				
			'Get the R/E (L85200) USD value from prior year Dec
			RE_Val_PY = HS.GetCell("P#Prior.A#L85200" & Tops & ".V#USD Total")
			
			'Clear the intersection and set the value for January
			HS.Clear "A#X280002" & Calc_Load
			HS.SetDataWithPOV "A#X280002" & Calc_Load, RE_Val_PY, FALSE
		
		'If Feb thru Dec...
		Else
	
			'Get the R/E (X280002) USD value from prior month
			RE_Val_PM = HS.GetCell("P#Prior.A#X280002" & Tops & ".V#USD Total")
			
			'Clear the intersection and set the value for the month
			HS.Clear "A#X280002" & Calc_Load
			HS.SetDataWithPOV "A#X280002" & Calc_Load, RE_Val_PM, FALSE
		
		End If
	End If
	
	'Added M-17-044 (for Pro Forma reporting)
	If pov_scenario = "Actual" Then
	
		'Only run on base entities and only run on Entity Currency
		If pov_year > "2013" And isABase = TRUE And pov_value = "<Entity Currency>" Then
		
			If pov_entity ="C_DAL" Then
			
				'Do Nothing if it is a CF entity
				HS.Clear "A#X280002" & Calc_Load_PRO
				
			Else
				
				'Set up the retained earnings beginning balance
				If pov_period = 1 Then

					HS.Clear "A#X280002" & Calc_Load_PRO
					HS.Exp "A#X280002" & Calc_Load_PRO & " = P#Prior.A#L85200" & Tops_AcPRO & source_value

				Else
					
					HS.Clear "A#X280002" & Calc_Load_PRO
					HS.Exp "A#X280002" & Calc_Load_PRO & " = P#Prior.A#X280002" & Tops_AcPRO & source_value

				End If
					
			End If

		End If

	End If
	
	
	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Earnings Per Share (EPS)                                                 *
	'*  Rule Number: C003                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*  	 Author: G. Martin                                                                *
	'* Date Created: 18Feb2016      		                                                  *
	'*                                                                                        *
	'*      Purpose: EPS Logic and Calculations for GAAP and Management views using basic     *
	'*				 and diluted share values for the numerator, GAAP uses total net income   *
	'*				 and Management uses net income excluding special items (C3:SP_Mgmt)  	  *
	'*                                                                                        *
	'*         Note: Added a section for "Diluted Shares Reported" in order to report out     *
	'*				 which shares were used as Diluted shares for a given period and income   *
	'*				 position. If net income is > zero, diluted are used (basic otherwise).   *
	'*         		 The entire EPS section of the rules was re-written during the addition   *
	'*				 of the Diluted Shares Reported section. The two calculations have been   *
	'*				 integrated so that the EPS calcs can leverage the DSR section.		      *
	'*                                                                                        *
	'*                                                                                        *
	'******************************************************************************************										
	
	'Added M-16-014
	'Diluted Shares Reported
	If pov_entity = "E_Global" And pov_value = "<Entity Currency>" Then ' run calc on E_Global only
			
		'Get Variables for Net Income
		NI_GAAP_MTD = HS.GetCell("E#P_DALINC.A#R99900.W#Periodic" & Tops)
		NI_GAAP_QTD = HS.GetCell("E#P_DALINC.A#R99900.W#QTD" & Tops)
		NI_GAAP_YTD = HS.GetCell("E#P_DALINC.A#R99900.W#YTD" & Tops)
		
		NI_MGMT_MTD = HS.GetCell("E#P_DALINC.A#R99900.W#Periodic" & Mgmt_Tops)
		NI_MGMT_QTD = HS.GetCell("E#P_DALINC.A#R99900.W#QTD" & Mgmt_Tops)
		NI_MGMT_YTD = HS.GetCell("E#P_DALINC.A#R99900.W#YTD" & Mgmt_Tops)
		
		'Get Variables for Shares that were input
		Basic_MTD = HS.GetCell("E#E_Global.A#SI_BasicMTD" & Share_Input)
		Basic_QTD = HS.GetCell("E#E_Global.A#SI_BasicQTD" & Share_Input)
		Basic_YTD = HS.GetCell("E#E_Global.A#SI_BasicYTD" & Share_Input)
		
		Diluted_MTD = HS.GetCell("E#E_Global.A#SI_DilutedMTD" & Share_Input)
		Diluted_QTD = HS.GetCell("E#E_Global.A#SI_DilutedQTD" & Share_Input)
		Diluted_YTD = HS.GetCell("E#E_Global.A#SI_DilutedYTD" & Share_Input)
		
		'Set GAAP Diluted Shares Reported
		If NI_GAAP_MTD >= 0 Then HS.SetDataWithPOV "A#SC_GDilRpt_MTD" & Share_Input, Diluted_MTD, FALSE
		If NI_GAAP_MTD < 0 Then HS.SetDataWithPOV "A#SC_GDilRpt_MTD" & Share_Input, Basic_MTD, FALSE
		
		If NI_GAAP_QTD >= 0 Then HS.SetDataWithPOV "A#SC_GDilRpt_QTD" & Share_Input, Diluted_QTD, FALSE
		If NI_GAAP_QTD < 0 Then HS.SetDataWithPOV "A#SC_GDilRpt_QTD" & Share_Input, Basic_QTD, FALSE
		
		If NI_GAAP_YTD >= 0 Then HS.SetDataWithPOV "A#SC_GDilRpt_YTD" & Share_Input, Diluted_YTD, FALSE
		If NI_GAAP_YTD < 0 Then HS.SetDataWithPOV "A#SC_GDilRpt_YTD" & Share_Input, Basic_YTD, FALSE
		
		'Set MGMT Diluted Shares Reported
		If NI_MGMT_MTD >= 0 Then HS.SetDataWithPOV "A#SC_MDilRpt_MTD" & Share_Input, Diluted_MTD, FALSE
		If NI_MGMT_MTD < 0 Then HS.SetDataWithPOV "A#SC_MDilRpt_MTD" & Share_Input, Basic_MTD, FALSE
		
		If NI_MGMT_QTD >= 0 Then HS.SetDataWithPOV "A#SC_MDilRpt_QTD" & Share_Input, Diluted_QTD, FALSE
		If NI_MGMT_QTD < 0 Then HS.SetDataWithPOV "A#SC_MDilRpt_QTD" & Share_Input, Basic_QTD, FALSE
		
		If NI_MGMT_YTD >= 0 Then HS.SetDataWithPOV "A#SC_MDilRpt_YTD" & Share_Input, Diluted_YTD, FALSE
		If NI_MGMT_YTD < 0 Then HS.SetDataWithPOV "A#SC_MDilRpt_YTD" & Share_Input, Basic_YTD, FALSE
		
		'Get Diluted Share Reported Variables (data just populated)
		DilRpt_GAAP_MTD = HS.GetCell("E#E_Global.A#SC_GDilRpt_MTD" & Share_Input)
		DilRpt_GAAP_QTD = HS.GetCell("E#E_Global.A#SC_GDilRpt_QTD" & Share_Input)
		DilRpt_GAAP_YTD = HS.GetCell("E#E_Global.A#SC_GDilRpt_YTD" & Share_Input)
		
		DilRpt_MGMT_MTD = HS.GetCell("E#E_Global.A#SC_MDilRpt_MTD" & Share_Input)
		DilRpt_MGMT_QTD = HS.GetCell("E#E_Global.A#SC_MDilRpt_QTD" & Share_Input)
		DilRpt_MGMT_YTD = HS.GetCell("E#E_Global.A#SC_MDilRpt_YTD" & Share_Input)
		
		
		'Make sure not to divide by zero
		If HS.IsZero(Basic_MTD) Then Basic_MTD = 1
		If HS.IsZero(Basic_QTD) Then Basic_QTD = 1
		If HS.IsZero(Basic_YTD) Then Basic_YTD = 1
		
		If HS.IsZero(DilRpt_GAAP_MTD) Then DilRpt_GAAP_MTD = 1
		If HS.IsZero(DilRpt_GAAP_QTD) Then DilRpt_GAAP_QTD = 1
		If HS.IsZero(DilRpt_GAAP_YTD) Then DilRpt_GAAP_YTD = 1
		
		If HS.IsZero(DilRpt_MGMT_MTD) Then DilRpt_MGMT_MTD = 1
		If HS.IsZero(DilRpt_MGMT_QTD) Then DilRpt_MGMT_QTD = 1
		If HS.IsZero(DilRpt_MGMT_YTD) Then DilRpt_MGMT_YTD = 1
		
		'Set EPS Values (using Diluted Reported shares)
		HS.SetDataWithPOV "A#SC_BGEPSMTD" & Calc_Load, (NI_GAAP_MTD / Basic_MTD), FALSE
		HS.SetDataWithPOV "A#SC_BGEPSQTD" & Calc_Load, (NI_GAAP_QTD / Basic_QTD), FALSE
		HS.SetDataWithPOV "A#SC_BGEPSYTD" & Calc_Load, (NI_GAAP_YTD / Basic_YTD), FALSE
		
		HS.SetDataWithPOV "A#SC_DGEPSMTD" & Calc_Load, (NI_GAAP_MTD / DilRpt_GAAP_MTD), FALSE
		HS.SetDataWithPOV "A#SC_DGEPSQTD" & Calc_Load, (NI_GAAP_QTD / DilRpt_GAAP_QTD), FALSE
		HS.SetDataWithPOV "A#SC_DGEPSYTD" & Calc_Load, (NI_GAAP_YTD / DilRpt_GAAP_YTD), FALSE
		
		HS.SetDataWithPOV "A#SC_BMEPSMTD" & Calc_Load, (NI_MGMT_MTD / Basic_MTD), FALSE
		HS.SetDataWithPOV "A#SC_BMEPSQTD" & Calc_Load, (NI_MGMT_QTD / Basic_QTD), FALSE
		HS.SetDataWithPOV "A#SC_BMEPSYTD" & Calc_Load, (NI_MGMT_YTD / Basic_YTD), FALSE
		
		HS.SetDataWithPOV "A#SC_DMEPSMTD" & Calc_Load, (NI_MGMT_MTD / DilRpt_MGMT_MTD), FALSE
		HS.SetDataWithPOV "A#SC_DMEPSQTD" & Calc_Load, (NI_MGMT_QTD / DilRpt_MGMT_QTD), FALSE
		HS.SetDataWithPOV "A#SC_DMEPSYTD" & Calc_Load, (NI_MGMT_YTD / DilRpt_MGMT_YTD), FALSE
		
	End If 'E_Global

