				'If isABase = TRUE Then

'					HS.Clear "A#OOB"
'					HS.Exp "A#OOB" & Calc_Load & " = A#A99900" & Tops & "- A#L99900" & Tops

				'End If

'		End If
			
'	End If

	
	
	
	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Calculate CTA on the Balance Sheet                                       *
	'*  Rule Number: C011                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G. Broadhurst                                                            *
	'* Date Created: 13May15                                                                  *
	'*                                                                                        *
	'*      Purpose: Calculate CTA on the Balance Sheet.  This rule will only run on          *
	'*               non-USD entities that are translating to USD				              *
	'*                                                                                        *
	'******************************************************************************************

	'Updated M-16-048
	If (pov_transadj = TRUE OR pov_trans = TRUE) and pov_scenario = "Actual" Then
	
		If isABase = TRUE Then
		
			HS.Clear "A#CTA_Calc"
			HS.Exp "A#CTA_Calc" & Calc_Load & " = A#A99900" & Tops & "- A#L99900" & Tops
		
		End If
		
	End If
	
	

End Sub		'Calculate Subroutine
