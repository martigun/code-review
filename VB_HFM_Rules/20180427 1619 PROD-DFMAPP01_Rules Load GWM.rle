		'BEGIN_FINANCE
		
		'CASH FLOWS FROM FINANCING ACTIVITIES

		If pov_entity = "C_DAL" Then

			'Payments on long-term debt and capital lease obligations

			'Early termination
			HS.Clear "A#CF91000" & RFO_CF_DL
			HS.Exp "A#CF91000" & RFO_CF_DL & " = A#SL57990.C3#Debt_Early_Termin.C4#Rollforward" & RFO_Tops_DL

			'Current debt
			HS.Clear "A#CF91200" & RFO_CF_DL
			HS.Exp "A#CF91200" & RFO_CF_DL & " = A#SL57990.C3#Debt_PrinPay.C4#Rollforward" & RFO_Tops_DL

			'Capital lease
			HS.Clear "A#CF91400" & RFO_CF_DL
			HS.Exp "A#CF91400" & RFO_CF_DL & " = A#SL58610.C3#CL_Cash_Pmt.C4#Rollforward" & RFO_Tops_DL & _
											 " + A#SL58610.C3#CL_POL.C4#Rollforward" & RFO_Tops_DL

			'Contra debt buyback
			HS.Clear "A#CF91600" & RFO_CF_DL
			HS.Exp "A#CF91600" & RFO_CF_DL & " = A#SL57990.C3#Debt_ConDebtBB.C4#Rollforward" & RFO_Tops_DL

			'Cash dividends
			HS.Clear "A#CF92000" & RFO_CF_DL
			HS.Exp "A#CF92000" & RFO_CF_DL & " = A#L42500.C3#Divid_PaidOut.C4#Rollforward" & RFO_Tops_DL			

			'Proceeds from short-term obligations
			HS.Clear "A#CF92200" & RFO_CF_DL
			HS.Exp "A#CF92200" & RFO_CF_DL & " = A#L00500.C3#Debt_Issuance.C4#Rollforward" & RFO_Tops_DL

			'Proceeds from long-term obligations
			'Updated M-18-002
			HS.Clear "A#CF92400" & RFO_CF_DL
			HS.Exp "A#CF92400" & RFO_CF_DL & " = A#SL56610.C3#Debt_Issuance.C4#Rollforward" & RFO_Tops_DL &_
												" + A#SL57710.C3#Debt_Issuance.C4#Rollforward" & RFO_Tops_DL

			'Debt issuance costs
			HS.Clear "A#CF92600" & RFO_CF_DL
			HS.Exp "A#CF92600" & RFO_CF_DL & " = - A#SA76210.C3#Debt_Issuance.C4#Rollforward" & RFO_Tops_DL & _
												" + A#SL55770.C3#Debt_Issuance.C4#Rollforward" & RFO_Tops_DL
									
			'Added M-16-054 (MRE)
			HS.Clear "A#CF92600" & RFO_CF_0700
			HS.Exp "A#CF92600" & RFO_CF_0700 & " = - A#SA76210.C3#Subs_DebtIssue.C4#Rollforward" & RFO_Tops_0700
			
			'Proceeds from exercise of stock options
			HS.Clear "A#CF92800" & RFO_CF_DL
			HS.Exp "A#CF92800" & RFO_CF_DL & " = A#SL92610.C3#Eqty_StkOptions.C4#Rollforward" & RFO_Tops_DL

			'Issuance of common stock
			HS.Clear "A#CF93000" & RFO_CF_DL
			HS.Exp "A#CF93000" & RFO_CF_DL & " = A#SL92610.C3#Eqty_IssCommStk.C4#Rollforward" & RFO_Tops_DL

			'Sale (repurchase) of common stock
			HS.Clear "A#CF93200" & RFO_CF_DL
			HS.Exp "A#CF93200" & RFO_CF_DL & " = A#SL92610.C3#Eqty_StkRepurch.C4#Rollforward" & RFO_Tops_DL

		End If

		'(Increase) decrease in financing restricted cash
		'Debt payment escrow
		' Removed M-18-001
		If pov_entity = "C_DAL" Then
			HS.Clear "A#CF93400" & RFO_CF_DL
			' HS.Exp "A#CF93400" & RFO_CF_DL & " = - A#A08000.C3#BS_Mvmt" & TopsNoC3 '(Change)
		End If

		'Fuel card obligation, net
		If pov_entity = "C_DAL" Then

			'Crude obligation
			HS.Clear "A#CF93800" & RFO_CF_DL
			HS.Exp "A#CF93800" & RFO_CF_DL & " = A#2190793.C3#BS_Mvmt" & TopsNoC3 '(Change)

			'Fuel obligation
			HS.Clear "A#CF94000" & RFO_CF_DL
			HS.Exp "A#CF94000" & RFO_CF_DL & " = A#2190794.C3#BS_Mvmt" & TopsNoC3 '(Change)

			'Other, net (Journal Entry)
		End If
		
		'Other financing activities, net
		'Added M-17-034
		If pov_entity = "C_DAL" Then

			'Airport construction reimbursements
			HS.Clear "A#CF97800" & RFO_CF_DL
			HS.Exp "A#CF97800" & RFO_CF_DL & " = A#SL47870.C3#FltGrd_Reimb.C4#Rollforward" & RFO_Tops_DL & _
											" + A#SL75470.C3#FltGrd_Reimb.C4#Rollforward" & RFO_Tops_DL
		End If
		
		'END_FINANCE
	
		'Beginning Cash Balance
		'If the period is Jan then we need to pull from Dec of the prior year, else we just need to pull
		'from the previous period. It is best to stay within the sub-cube (Year, Scenario, Entity) for
		'better performance.

		' If pov_entity = "C_DAL" Then
			
			' HS.Clear "A#CF99910" & CF_Calcs
			
			' ' If PeriodIsFirst = TRUE Then
				' ' HS.Exp "A#CF99910" & CF_Calcs & " = A#A03500.P#LAST.Y#PRIOR" & Tops '(Change)
			' ' Else
				' ' HS.Exp "A#CF99910" & CF_Calcs & " = A#CF99910.P#PRIOR" & Tops '(Change)
			' ' End If

		' End If
	
	End If 'If pov_scenario = "Actual" Then
	End If 'If pov_year > 2013 Then 
	
	'Ending Cash Balance
	'Updated M-18-001
	If pov_entity = "C_DAL" And pov_scenario = "Actual" Then
	
		'cash flow done by journal entry
		If pov_year = "2014" Then
	
			'Set ending cash balance equal to the balance sheet at this period
			HS.Clear "A#CF99920" & RFO_CF_DL
			HS.Exp "A#CF99920" & RFO_CF_DL & " = A#A03500.E#P_DALINC" & Tops
		End If
		
		'cash flow rules take effect
		If pov_year > "2014" Then
			
			'(includes) restricted cash
			'Set ending cash balance equal to the balance sheet at this period
			HS.Clear "A#CF99920" & RFO_CF_DL
			HS.Exp "A#CF99920" & RFO_CF_DL & " = A#SA08570.E#P_DALINC" & Tops
		End If
	End If
	
	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Historical Dollar Overrides				                              *
	'*  Rule Number: C014                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G.Broadhurst                                                             *
	'* Date Created: 26Oct15                                                                  *
	'* Date Updated: 25Jan16 by G. Martin                                                     *
	'*                                                                                        *
	'*      Purpose: There are Override accounts set up in HFM to account for historical      *
	'*		 		 amounts in Equity and Investments. If the entity is translating to       *
	'*		 		 another currency then we may have an override amount. This rule will     * 
	'*		 		 run for all base foreign company entities that translate to USD.         *
	'*                                                                                        *
	'*               This rule is designed to take the input value of override (OVR)          *
	'*               account as a change in balance of the account in the local			      *
	'*               currency. The input to the OVR account should be done via HFM journal.   *
	'*                                                                                        *
	'******************************************************************************************
	
	'Transferring USD Override amounts in Account Dimension (start running in 2016)
	If pov_scenario = "Actual" and pov_year > "2015" Then
	
		'If the entity translates and the Value dimension member is USD
	 	If pov_trans = TRUE and pov_value = "USD" Then
				
			'For each override, we first check to see if there is an amount in the override (OVR) account
			'If there is then we add that translated amount to the prior period USD balance
			
			'Establish list of variables to hold the OVR account list
			vOverrides = HS.Account.List("Overrides","[Base]")
			
			'Establish a variable for prior period
			If PeriodIsFirst = TRUE Then Prior_Period = ".Y#PRIOR.P#LAST"
			If PeriodIsFirst = FALSE Then Prior_Period = ".P#PRIOR"

			'Loop through the OVR account list
			For i = LBound(vOverrides) to UBound(vOverrides)
			
				'Set a variable to hold the "true" account number
				'by stripping away the 'OVR_' in front of each of the OVR accounts
				vAccount = Right(vOverrides(i), Len(vOverrides(i)) - 4)
			
				'Set the variable vOvr_IsZero to TRUE if the OVR account is zero
				vOvr_IsZero = HS.IsZero(HS.GetCell("A#" & vOverrides(i) & Tops))
				
				'First check to see if period is Jan 2016
				If PeriodIsFirst = TRUE and pov_year = "2016" Then
				
					'For Jan 2016, move translated OVR into the "true" account
					HS.Exp "A#" & vAccount & SAP_BalSht & "= A#" & vOverrides(i) & Tops

				'Evaluate whether the amount in the OVR account is NOT zero
				ElseIf vOvr_IsZero = FALSE Then
				
					'If OVR is NOT zero, add this translated OVR into the "true" account from Prior Period
					HS.Exp "A#" & vAccount & SAP_BalSht & "= A#" & vOverrides(i) & Tops & _
							" + A#" & vAccount & Tops & Prior_Period
				Else 
				
					'If OVR account is zero, set the "true" account equal to the prior period
     				HS.Exp "A#" & vAccount & SAP_BalSht & "= A#" & vAccount & Tops & Prior_Period
  				End If
				
			Next 'Next OVR account
			
		End If	'If the entity translates and the Value dimension member is USD
	End If	'If pov_scenario = "Actual" Then
	
	
	'******************************************************************************************
	'*                                                                                        *
	'*    Rule Name: Balance Sheet Out of Balance (OOB)                                       *
	'*  Rule Number: C004                                                                     *
	'*   Subroutine: Calculate                                                                *
	'*       Author: G. Broadhurst                                                            *
	'* Date Created: 13Apr2015                                                                *
	'*                                                                                        *
	'*      Purpose: To calculate the Out of Balance (OOB) Account in the Balance Sheet, 	  *
	'*				 which is Assets minus Liabilities minus Equity							  *
	'*																						  *
	'*		Note: We DO want this to fire for C_DAL, since OOB won't copy					  *
	'*                                                                                        *
	'******************************************************************************************

	'Updated M-16-048
	If pov_scenario = "Actual" Then
	
		If pov_value = "<Entity Currency>" or pov_value = "<Entity Curr Adjs>" Then

			If isABase = TRUE Then

				HS.Clear "A#OOB"
				HS.Exp "A#OOB" & Calc_Load & " = A#A99900" & Tops & "- A#L99900" & Tops

			End If

		End If
		
	End If


'	If pov_entity = "P_Intl_Branches" Then
			
